{
  "routes": [
    {
      "route": "/",
      "serve": "/index.html"
    },
    {
      "route": "/*",
      "serve": "/index.html",
      "statusCode": 404
    }
  ],
  "navigationFallback": {
    "rewrite": "/index.html",
    "exclude": ["/assets/*", "/*.{css,scss,js,png,gif,ico,jpg,svg}"]
  },
  "mimeTypes": {
    ".svg": "image/svg+xml"
  },
  "globalHeaders": {
    "Cache-Control": "public, max-age=31536000"
  },
  "responseOverrides": {
    "404": {
      "rewrite": "/404.html",
      "statusCode": 404
    }
  }
}